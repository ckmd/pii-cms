<div class="row">

    <!-- Blog Entries Column -->
    <div class="col-md-8 box" style="background-color: #fff;">
        <!-- Blog Post -->
        {{!-- {{#each posts}} --}}
            {{!-- <img width="100%" src="{{post.file}}" alt="Card image cap"> --}}
            <iframe width="100%" height="360" frameborder="0" allow="accelerometer; autoplay; encrypted_media; gyroscope; picture-in-picture" allowfullscreen
                src="https://www.youtube.com/embed/Cm9ZkYTnCNE">
            </iframe>
            <div class="card mb-4">
                <div id="labelboxvideo">
                    <p class="card-text">{{post.status}}</p>
                </div>
                <div class="card-body">
                    <h1 class="card-title video">{{post.title}}</h1>
                    <p class="card-text">{{post.description}}</p>
                </div>
            </div>
        {{!-- {{/each}} --}}

    </div>

    {{> 'default/sidebar'}}
    <!-- Sidebar Widgets Column -->

</div>
<!-- /.row -->
