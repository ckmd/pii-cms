<div class="row" style="background-color: #fff; font-family: 'Montserrat';">
    <!-- Blog Entries Column -->
    <div id="labelall">
        <p class="card-text">{{child.title}}</p>
    </div>
    <div class="row" style="padding: 0px 60px;">
        {{!-- <div class="card"> --}}
            <div class="card-body">
                <p class="card-text">
                    {{{child.description}}}
                </p>
            </div>
        {{!-- </div> --}}
    </div>
    <div class="row" style="padding: 30px 60px">
        <!-- Blog Post -->
        {{#each pengurus}}
            {{#ifCond @index '==' 0}}
            <div class="col-md-12 text-center leader-pict">
                <img src="{{this.file}}" width="20%" alt="" style="border-radius: 50%; margin-bottom:15px;">
                <span class="tooltiptext">{{this.deskripsi}}</span>
                <strong><h5 style="font-family: 'Montserrat';">{{this.nama}}</h5></strong>
                <p>{{this.jabatan}}</p>
            </div>
            {{/ifCond}}
            {{#ifCond @index '>' 0}}
                <div class="member-pict{{#moduloIf @index 5 0}}-left{{/moduloIf}}">
                    <img src="{{this.file}}" width="100%" alt="" style="border-radius: 50%; margin-bottom:10px;">
                    <span class="tooltiptext">{{this.deskripsi}}</span>
                    <strong><p>{{this.nama}}</p></strong>
                    <p>{{this.jabatan}}</p>
                </div>
            {{/ifCond}}
        {{/each}}
    </div>
</div>
